            "Andmarruda\\Asaas\\Controllers": "./Controllers/",
            "Andmarruda\\Asaas\\Events": "./Events/",
            "Andmarruda\\Asaas\\Listeners": "./Listeners/",
            "Andmarruda\\Asaas\\Migrations": "./Migrations/",
            "Andmarruda\\Asaas\\Models": "./Models/",
            "Andmarruda\\Asaas\\Providers": "./Providers/",
            "Andmarruda\\Asaas\\Tests": "./Tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andmarruda\\Asaas\\AndmarrudaAssasProvider"
            ]
        }
    }
